/*9. Write a recursive function to print octal of a given decimal number*/

#include<stdio.h>
void d_to_c(int n);// function prototype
int main()
{
int n;
// input number from user 
printf("Enter the decimal number :");
scanf("%d",&n);
d_to_c(n); // function calling
return 0;
}
void d_to_c(int n){ // function defination
    if(n==0){
        return;
    }
    else{
    d_to_c(n/8);
    printf("%d",n%8);
    }
   
}
